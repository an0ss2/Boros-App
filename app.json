{
  "expo": {
    "name": "Boros AI Workspace",
    "slug": "boros-ai-workspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "boros",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "primaryColor": "#C15F3C",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.boros.aiworkspace"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#C15F3C"
      },
      "package": "com.boros.aiworkspace"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "boros-ai-workspace"
      }
    }
  }
}
